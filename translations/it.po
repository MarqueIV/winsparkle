#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Aggiornamento software"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Note di rilascio:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Salta questa versione"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Ricordamelo più tardi"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Installa aggiornamento"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s non può essere riavviato."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Assicurati di non avere documenti non salvati e riprova."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Avvio installazione…"

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Impossibile avviare l'installazione."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Controllo aggiornamenti…"

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Annulla"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Sei aggiornato!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s è l'ultima versione disponibile."

#: src/ui.cpp:774
msgid "Close"
msgstr "Chiudi"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Errore aggiornamento."

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Si è verificato un errore durante il recupero delle informazioni di aggiornamento. Sei connesso a Internet? Riprova più tardi."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "L'aggiornamento è firmato impropriamente."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Una nuova versione di %s è disponibile!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Aggiorna"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s è ora disponibile (hai %s). Vuoi scaricarlo ora?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Scaricamento aggiornamento…"

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s di %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Pronto per l'installazione."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Controllare aggiornamenti automaticamente?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Lasciare a %s il controllo automatico degli aggiornamenti? Puoi sempre controllare manualmente dal menu'."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Controlla automaticamente"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Non controllare"

