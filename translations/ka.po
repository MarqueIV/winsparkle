#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "პროგრამის განახლება"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "განახლების აღწერა:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "ამ ვერსიის გამოტოვება"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "მოგვიანებით შეხსენება"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "განახლების დაყენება"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "%s-ის გადატვირთვა შეუძლებელია."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "გთხოვთ, დარწმუნდეთ რომ არ გაქვთ შეუნახავი დოკუმენტები და სცადოთ ხელახლა."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "მიმდინარეობს დაყენების დაწყება..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "დაყენების დაწყება ვერ მოხერხდა."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "მიმდინარეობს განახლებების შემოწმება..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "გაუქმება"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "თქვენი ვერსია განახლებულია!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s ამჟამად არის უახლესი ვერსია."

#: src/ui.cpp:774
msgid "Close"
msgstr "დახურვა"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "განახლების შეცდომა!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "განახლების ინფორმაციის მიღებისას წარმოიშვა შეცდომა; ხართ თუ არა დაკავშირებული ინტერნეტთან? გთხოვთ, სცადოთ მოგვიანებით."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "განახლება არ არის სწორად ხელმოწერილი."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "ხელმისაწვდომია %s-ის ახალი ვერსია!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "განახლების მიღება"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "ხელმისაწვდომია %s %s (თქვენი ვერსიაა %s). გსურთ ჩამოტვირთოთ ახლა?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "მიმდინარეობს განახლების ჩამოტვირთვა..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s %s-დან"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "მზად არის დასაყენებლად."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "მოხდეს განახლების ავტომატური შემოწმება?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "გსურთ რომ %s-მა ავტომატურად შეამოწმოს განახლებები? თქვენ ყოველთვის შეგიძლიათ ხელით შეამოწმოთ განახლებები მენიუდან."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "ავტომატურად შემოწმება"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "არ მოხდეს შემოწმება"

