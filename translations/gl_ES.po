#: src/ui.cpp:325 src/ui.cpp:668 src/ui.cpp:684
msgid "Software Update"
msgstr "Actualización do soporte lóxico"

#: src/ui.cpp:513
msgid "Release notes:"
msgstr "Novidades:"

#: src/ui.cpp:539
msgid "Skip this version"
msgstr "Ignorar esta versión"

#: src/ui.cpp:545
msgid "Remind me later"
msgstr "Lembrarmo máis tarde"

#: src/ui.cpp:550 src/ui.cpp:563
msgid "Install update"
msgstr "Instalar actualización"

#: src/ui.cpp:667
#, c-format
msgid "%s cannot be restarted."
msgstr "non se pode reiniciar %s."

#: src/ui.cpp:670
msgid "Make sure that you don't have any unsaved documents and try again."
msgstr "Teña a certeza de que non ten ningún documento sen gardar e inténteo de novo."

#: src/ui.cpp:677
msgid "Launching the installer..."
msgstr "Iniciando o instalador..."

#: src/ui.cpp:683
msgid "Failed to launch the installer."
msgstr "Houbo un erro ao iniciar o instalador."

#: src/ui.cpp:726
msgid "Checking for updates..."
msgstr "Buscando actualizacións..."

#: src/ui.cpp:728 src/ui.cpp:809 src/ui.cpp:897
msgid "Cancel"
msgstr "Cancelar"

#: src/ui.cpp:754
msgid "You're up to date!"
msgstr "Xa está actualizado!"

#: src/ui.cpp:761
#, c-format
msgid "%s %s is currently the newest version available."
msgstr "%s %s é a versión dispoñible máis recente."

#: src/ui.cpp:774
msgid "Close"
msgstr "Pechar"

#: src/ui.cpp:795
msgid "Update Error!"
msgstr "Houbo un erro ao actualizar!"

#: src/ui.cpp:801
msgid "An error occurred in retrieving update information; are you connected to the internet? Please try again later."
msgstr "Ocorreu un erro ao querer obter información sobre a actualización. Está conectado á rede? Inténteo novamente máis tarde."

#: src/ui.cpp:804
msgid "The update is improperly signed."
msgstr "A actualización non está asinada correctamente."

#: src/ui.cpp:855
#, c-format
msgid "A new version of %s is available!"
msgstr "Está dispoñible unha nova versión de %s!"

#: src/ui.cpp:858
msgid "Get update"
msgstr "Baixar actualización"

#: src/ui.cpp:864
#, c-format
msgid "%s %s is now available (you have %s). Would you like to download it now?"
msgstr "%s %s está dispoñible (a súa versión é a %s). Gustaríache baixar a nova versión agora?"

#: src/ui.cpp:895
msgid "Downloading update..."
msgstr "Descargando actualización..."

#. TRANSLATORS: This is the progress of a download, e.g. "3 MB of 12 MB".
#: src/ui.cpp:923
#, c-format
msgid "%s of %s"
msgstr "%s de %s"

#: src/ui.cpp:960
msgid "Ready to install."
msgstr "Listo para instalar."

#: src/ui.cpp:1105
msgid "Check for updates automatically?"
msgstr "Buscar actualizacións automaticamente?"

#: src/ui.cpp:1115
#, c-format
msgid "Should %s automatically check for updates? You can always check for updates manually from the menu."
msgstr "Debe %s comprobar as actualizacións automaticamente? Podes comprobar as actualizacións manualmente desde o menú."

#: src/ui.cpp:1129
msgid "Check automatically"
msgstr "Buscar automaticamente"

#: src/ui.cpp:1134
msgid "Don't check"
msgstr "Non comprobar"

